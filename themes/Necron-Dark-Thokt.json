{
  "name": "Necron Dark - Thokt",
  "type": "dark",
  "colors": {
    "editor.background": "#101010",
    "editor.foreground": "#E0E0E0",
    "editorCursor.foreground": "#7DF9FF",
    "editor.lineHighlightBackground": "#1A1A1A",
    "editor.selectionBackground": "#00334D",
    "editor.wordHighlightBackground": "#002233",
    "sideBar.background": "#181818",
    "statusBar.background": "#202020",
    "statusBar.foreground": "#7DF9FF",
    "activityBar.background": "#181818",
    "tab.activeBackground": "#202020",
    "tab.inactiveBackground": "#181818",
    "tab.border": "#1F1F1F"
  },
  "tokenColors": [
    {
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#7DF9FF"
      }
    },
    {
      "scope": ["string", "variable.other.readwrite"],
      "settings": {
        "foreground": "#A0FFFF"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#5C6C70",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#88C0FF"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#8FD8FF"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#80A0FF"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#FF5A5A",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["warning"],
      "settings": {
        "foreground": "#FFD700"
      }
    }
  ]
}
