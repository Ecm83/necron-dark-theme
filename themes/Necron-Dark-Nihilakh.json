{
  "name": "Necron Dark - Nihilakh",
  "type": "dark",
  "colors": {
    "editor.background": "#1A1A1A",
    "editor.foreground": "#E0E0E0",
    "editorCursor.foreground": "#40E0D0",
    "editor.lineHighlightBackground": "#262626",
    "editor.selectionBackground": "#1A4D4D",
    "editor.wordHighlightBackground": "#113333",
    "sideBar.background": "#1F1F1F",
    "statusBar.background": "#262626",
    "statusBar.foreground": "#40E0D0",
    "activityBar.background": "#1F1F1F",
    "tab.activeBackground": "#262626",
    "tab.inactiveBackground": "#1F1F1F",
    "tab.border": "#333333"
  },
  "tokenColors": [
    {
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#40E0D0"
      }
    },
    {
      "scope": ["string", "variable.other.readwrite"],
      "settings": {
        "foreground": "#66FFFF"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#5C7070",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#88FFFF"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#66CCCC"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#80D0FF"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#FF5A5A",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["warning"],
      "settings": {
        "foreground": "#FFD700"
      }
    }
  ]
}
