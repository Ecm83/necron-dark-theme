{
  "name": "Necron Dark - Nephrekh",
  "type": "dark",
  "colors": {
    "editor.background": "#3B240B",
    "editor.foreground": "#F0E0C0",
    "editorCursor.foreground": "#00FF7F",
    "editor.lineHighlightBackground": "#4D3311",
    "editor.selectionBackground": "#665522",
    "editor.wordHighlightBackground": "#4D4411",
    "sideBar.background": "#2E1A00",
    "statusBar.background": "#4D2E00",
    "statusBar.foreground": "#00FF7F",
    "activityBar.background": "#2E1A00",
    "tab.activeBackground": "#4D2E00",
    "tab.inactiveBackground": "#2E1A00",
    "tab.border": "#664411"
  },
  "tokenColors": [
    {
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#00FF7F"
      }
    },
    {
      "scope": ["string", "variable.other.readwrite"],
      "settings": {
        "foreground": "#66FFB2"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#70685C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#FFEE99"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#FFDD88"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#FF3333",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["warning"],
      "settings": {
        "foreground": "#FFA500"
      }
    }
  ]
}
