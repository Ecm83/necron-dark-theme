{
  "name": "Necron Dark - Novokh",
  "type": "dark",
  "colors": {
    "editor.background": "#1A1A1A",
    "editor.foreground": "#E0E0E0",
    "editorCursor.foreground": "#FF4444",
    "editor.lineHighlightBackground": "#262626",
    "editor.selectionBackground": "#4D1A1A",
    "editor.wordHighlightBackground": "#331111",
    "sideBar.background": "#1F1F1F",
    "statusBar.background": "#262626",
    "statusBar.foreground": "#FF4444",
    "activityBar.background": "#1F1F1F",
    "tab.activeBackground": "#262626",
    "tab.inactiveBackground": "#1F1F1F",
    "tab.border": "#333333"
  },
  "tokenColors": [
    {
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#FF4444"
      }
    },
    {
      "scope": ["string", "variable.other.readwrite"],
      "settings": {
        "foreground": "#FF7777"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#705C5C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FFAA88"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#FF8888"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#FFCCCC"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#FF0000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["warning"],
      "settings": {
        "foreground": "#FFDD00"
      }
    }
  ]
}
