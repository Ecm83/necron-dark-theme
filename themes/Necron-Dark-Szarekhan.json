{
  "name": "Necron Dark - Szarekhan",
  "type": "dark",
  "colors": {
    "editor.background": "#181818",
    "editor.foreground": "#D0D0D0",
    "editorCursor.foreground": "#88FF88",
    "editor.lineHighlightBackground": "#232323",
    "editor.selectionBackground": "#2A2A2A",
    "editor.wordHighlightBackground": "#1F1F1F",
    "sideBar.background": "#1C1C1C",
    "statusBar.background": "#232323",
    "statusBar.foreground": "#DAA520",
    "activityBar.background": "#1C1C1C",
    "tab.activeBackground": "#232323",
    "tab.inactiveBackground": "#1C1C1C",
    "tab.border": "#2A2A2A"
  },
  "tokenColors": [
    {
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#88FF88"
      }
    },
    {
      "scope": ["string", "variable.other.readwrite"],
      "settings": {
        "foreground": "#B2FFB2"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#6C6C5C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#DAA520"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#E0C080"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#FF4040",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["warning"],
      "settings": {
        "foreground": "#FFA500"
      }
    }
  ]
}
