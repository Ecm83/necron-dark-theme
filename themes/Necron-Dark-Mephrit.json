{
  "name": "Necron Dark - Mephrit",
  "type": "dark",
  "colors": {
    "editor.background": "#2B1900",
    "editor.foreground": "#D0D0D0",
    "editorCursor.foreground": "#00FF00",
    "editor.lineHighlightBackground": "#332200",
    "editor.selectionBackground": "#664400",
    "editor.wordHighlightBackground": "#4D3300",
    "sideBar.background": "#1F1400",
    "statusBar.background": "#261900",
    "statusBar.foreground": "#00FF00",
    "activityBar.background": "#1F1400",
    "tab.activeBackground": "#261900",
    "tab.inactiveBackground": "#1F1400",
    "tab.border": "#332200"
  },
  "tokenColors": [
    {
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#00FF00"
      }
    },
    {
      "scope": ["string", "variable.other.readwrite"],
      "settings": {
        "foreground": "#66FF66"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#5C705C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FFCC00"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#FFCC66"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#FF9966"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#FF3333",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["warning"],
      "settings": {
        "foreground": "#FFD700"
      }
    }
  ]
}
