{
  "name": "Necron Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#101010",
    "editor.foreground": "#D0D0D0",
    "editorCursor.foreground": "#88FF88",
    "editor.lineHighlightBackground": "#1A1A1A",
    "editor.selectionBackground": "#2A7A2A",
    "editor.wordHighlightBackground": "#294229",
    "sideBar.background": "#181818",
    "statusBar.background": "#202020",
    "statusBar.foreground": "#88FF88",
    "activityBar.background": "#181818",
    "tab.activeBackground": "#202020",
    "tab.inactiveBackground": "#181818",
    "tab.border": "#1F1F1F"
  },
  "tokenColors": [
    {
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#88FF88"
      }
    },
    {
      "scope": ["string", "variable.other.readwrite"],
      "settings": {
        "foreground": "#B2FFB2"
      }
    },
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#5C705C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#88D8FF"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#8FD8FF"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#80A0FF"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#FF5A5A",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["warning"],
      "settings": {
        "foreground": "#FFD700"
      }
    }
  ]
}
